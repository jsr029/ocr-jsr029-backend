{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js",
        "methods": ["GET", "POST", "PATCH", "DELETE"],
        "headers": {
          "Access-Control-Allow-Methods": "GET, POST, PATCH, DELETE",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      },
		{
			"src": "^/(.*)",
			"dest": "/"
		}
    ]
}