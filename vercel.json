{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { 
      "source": "/assets", 
      "destination": "https://ocr-jsr029-backend.vercel.app/assets" 
    }
  ], 
  "static": { "directory": "assets"}
}

