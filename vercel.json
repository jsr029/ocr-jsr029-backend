{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "rewrites": [
    { 
      "source": "/assets/:path*", 
      "destination": "https://ocr-jsr029-backend.vercel.app/assets/:path*" 
    }
  ]
}

